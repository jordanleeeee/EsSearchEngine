    <style>
        /* Always set the map height explicitly to define the size of the div
         * element that contains the map. */
        #map {
            width: 100%;
            height: 600px;
        }
    </style> 
    <script>
        //console.log(areasGrouped);
        //console.log(stores);
        //console.log(storesByArea);


        var map;

        var markersArray = [];   //array store the pushed marker for clear up

        function initMap() {
            var start_position = {lat: 22.37, lng: 114.09};
            map = new google.maps.Map(document.getElementById('map'), {
                zoom: 10,
                center: start_position
            });

            jQuery("#search_btn").trigger('click');

            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(showPosition, errorHandler, {
                    enableHighAccuracy: false,
                    maximumAge: 60000,
                    timeout: 27000
                });
            }

        }


        function errorHandler() {

        }


        function showPosition(position) {
            console.log(position);

            location.latitude = position.coords.latitude;
            location.longitude = position.coords.longitude;

            var geocoder = new google.maps.Geocoder();
            var latLng = new google.maps.LatLng(location.latitude, location.longitude);

            if (geocoder) {
                geocoder.geocode({'latLng': latLng}, function (results, status) {

                    var position = {lat: parseFloat(location.latitude), lng: parseFloat(location.longitude)};
                    var marker;

                    marker = new google.maps.Marker({
                        position: position,
                        title: '現在位置 Current',
                        animation: google.maps.Animation.DROP,
                        icon: 'http://maps.google.com/mapfiles/ms/icons/green-dot.png',
                    });

                    var contentString = '<div style="font-family:Calibri, sans-serif;color:#54a42e;font-size: 16px;margin: 1px 0">' + '現在位置 Current : <br>' + results[0].formatted_address + '</div>';

                    var infowindow = new google.maps.InfoWindow({
                        content: contentString
                    });

                    infowindow.open(map, marker);

                    marker.setMap(map);
                    markersArray.push(marker);


                    if (status == google.maps.GeocoderStatus.OK) {
                        console.log(results[0].formatted_address);
                    }
                    else {
                        console.log("Geocoding failed: " + status);
                    }
                }); //geocoder.geocode()
            }
        }


        function addMarker(object) {


            /*if (!'lat' in object['location'] || !'lng' in object['location']) {
             return;
             }*/

            if (object['location'] == null) {
                return;
            }

            //console.log();

            var position = {lat: parseFloat(object['location']['lat']), lng: parseFloat(object['location']['lng'])};

            //console.log(position);

            var marker;

            marker = new google.maps.Marker({
                position: position,
                title: object['name'],
                animation: google.maps.Animation.DROP,
            });

            //the html content of the info windows
            var contentString = '<div style="font-family:Calibri, sans-serif;color:#54a42e;font-size: 26px;margin: 7px 0">' + object['name'] + '</div>' +
                '<div style="font-size: 16px;font-family:Calibri, sans-serif;margin:5px 0;">' + object['address'] + '</div>' +
                '<div style="font-size: 16px;font-family:Calibri, sans-serif;margin:5px 0;">' + object['open_hours'] + '</div>';

            var infowindow = new google.maps.InfoWindow({
                content: contentString
            });

            marker.addListener('click', function () {
                infowindow.open(map, marker);
            });

            marker.setMap(map);
            markersArray.push(marker);
        }


        function showShopList(object, counter) {

            if (object['location'] == null) {

                console.log('return');
                console.log(object);
                return;
            }


            var contentString = "";

            if (window.location.href.indexOf("/zh-hant/") != -1) {
                contentString = '<div class="shop-item"> <div class="left"> <div> <span class="num">' + counter + '</span> <span class="title">' + object['name'] + '</span> </div> <div class="info"> <div class="row"><a target="_blank" href="https://www.google.com/maps/?q=' + object['location'].lat + ',' + object['location'].lng + '">' + object['address'] + '</a></div> <div class="row">Tel: ' + object['phone'] + '</div> <div class="row"></div> </div> </div> <div class="right"> <div class="title"> 開放時間 </div> <div class="info"> <div>' + object['open_hours'] + '</div> </div> </div> <br class="clear"> </div>';
            } else if (window.location.href.indexOf("/zh-hans/") != -1) {
                contentString = '<div class="shop-item"> <div class="left"> <div> <span class="num">' + counter + '</span> <span class="title">' + object['name'] + '</span> </div> <div class="info"> <div class="row"><a target="_blank" href="https://www.google.com/maps/?q=' + object['location'].lat + ',' + object['location'].lng + '">' + object['address'] + '</a></div> <div class="row">Tel: ' + object['phone'] + '</div> <div class="row"></div> </div> </div> <div class="right"> <div class="title"> 开放时间 </div> <div class="info"> <div>' + object['open_hours'] + '</div> </div> </div> <br class="clear"> </div>';
            } else {
                contentString = '<div class="shop-item"> <div class="left"> <div> <span class="num">' + counter + '</span> <span class="title">' + object['name'] + '</span> </div> <div class="info"> <div class="row"><a target="_blank" href="https://www.google.com/maps/?q=' + object['location'].lat + ',' + object['location'].lng + '">' + object['address'] + '</a></div> <div class="row">Tel: ' + object['phone'] + '</div> <div class="row"></div> </div> </div> <div class="right"> <div class="title"> Open Hours </div> <div class="info"> <div>' + object['open_hours'] + '</div> </div> </div> <br class="clear"> </div>';
            }


            jQuery('.shop-list-container').append(contentString);
        }

        function clearShopList() {
            jQuery('.shop-list-container').html('<h2 class="title"></h2>');
        }


        jQuery("#search_btn").click(function () {
            //when the user click the search button

            if (jQuery("#district_list").val() != '') {
                //if user have selected the district
                clearOverlays();
                clearShopList();
                jQuery('.shop-list-container h2.title').html(areasGrouped[jQuery("#area_list").val()][jQuery("#district_list").val()]);
                //clear all marker before add marker
                var obj = storesByArea[jQuery("#district_list").val()];
                var counter = 1;
                var markers = [];

                for (var prop in obj) {
                    if (obj.hasOwnProperty(prop)) {
                        if (!markers[stores[obj[prop]]['id']]) {
                            markers[stores[obj[prop]]['id']] = true;
                            addMarker(stores[obj[prop]]);
                            showShopList(stores[obj[prop]], counter);
                            counter++;
                        }
                    }
                }

            } else if (jQuery("#area_list").val() != '') {
                //if user only selected the area
                var area_obj = areasGrouped[jQuery("#area_list").val()];
                clearOverlays();
                clearShopList();
                jQuery('.shop-list-container h2.title').html(areasGrouped[0][jQuery("#area_list").val()]);
                //clear all marker before add marker
                var area_counter = 1;
                var markers = [];


                for (var dis_prop in area_obj) {
                    if (storesByArea.hasOwnProperty(jQuery("#area_list").val())) {
                        var dis_obj = storesByArea[dis_prop];
                        for (var shop_prop in dis_obj) {
                            if (dis_obj.hasOwnProperty(shop_prop)) {
                                //console.log(shop_prop);
                                if (!markers[stores[dis_obj[shop_prop]]['id']]) {
                                    markers[stores[dis_obj[shop_prop]]['id']] = true;
                                    addMarker(stores[dis_obj[shop_prop]]);
                                    showShopList(stores[dis_obj[shop_prop]], area_counter++);
                                    console.log(area_counter);
                                }
                            }
                        }
                    }
                }

            } else {

                var area_obj = [];

                jQuery("#area_list option").each(function () {
                    area_obj.push(jQuery(this).val());
                });


                clearOverlays();
                clearShopList();
                jQuery('.shop-list-container h2.title').html(areasGrouped[0][jQuery("#area_list").val()]);
                //clear all marker before add marker
                var area_counter = 1;
                var markers = [];


                area_obj.forEach(function (entry) {
                    if (entry != '') {
                        for (var dis_prop in areasGrouped[entry]) {
                            var dis_obj = storesByArea[dis_prop];
                            for (var shop_prop in dis_obj) {
                                if (dis_obj.hasOwnProperty(shop_prop)) {
                                    //console.log(shop_prop);
                                    if (!markers[stores[dis_obj[shop_prop]]['id']]) {
                                        markers[stores[dis_obj[shop_prop]]['id']] = true;
                                        addMarker(stores[dis_obj[shop_prop]]);
                                        showShopList(stores[dis_obj[shop_prop]], area_counter++);
                                    }
                                }
                            }
                        }
                    }
                })
            }
        });

        //clear all marker
        function clearOverlays() {
            for (var i = 0; i < markersArray.length; i++) {
                markersArray[i].setMap(null);
            }
            markersArray.length = 0;
        }

    </script> <!-- script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAbnV29LbbCyiZevilZNU7koU2z52aBj4c&callback=initMap">
    </script --> 
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDGva5KelfrFRaunCQf3u2IT4le-DRVvcw&amp;callback=initMap">
    </script> 
   </div><!-- #content --> 

   <script>
    var map;
    var icon = 'https://www.mcdonalds.com.hk/wp-content/themes/mcdonald/images/google_map_pin_small.png';
    function initMap() {
        var mapOptions;
        mapOptions = {
            zoom: 12,
            minZoom: 3,
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            scrollwheel: false,
            panControl: false,
            zoomControl: true,
            mapTypeControl: false,
            scaleControl: false,
            streetViewControl: false,
            overviewMapControl: false,
            indoormaps: false,
            //draggable: false,
        };
        map = new google.maps.Map(document.getElementById("map"), mapOptions);
    }

</script> 
   <script src="https://maps.googleapis.com/maps/api/js?language=en&amp;key=AIzaSyAWxqHk7PX-0DUANo1zHHr_ry8C4Czp_XI&amp;callback=initMap"></script> 
